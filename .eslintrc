{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "no-console": 0,
    "no-multi-assign": 0,
    "no-plusplus": 0,
    "no-shadow": 0,
    "no-nested-ternary": 0,
    "no-underscore-dangle": 0,
    "no-prototype-builtins": 0
  }
}